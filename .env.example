# DataScout Environment Configuration
# Copy this file to .env and update with your values

# API Configuration
GOOGLE_API_KEY=your_google_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# CORS Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Debug Mode
DEBUG=true

# Database Configuration (if using database instead of in-memory storage)
# DATABASE_URL=sqlite:///./datascout.db

# File Upload Configuration
MAX_FILE_SIZE=100MB
UPLOAD_DIR=/app/uploads

# Report Generation
REPORT_CACHE_TTL=3600

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json